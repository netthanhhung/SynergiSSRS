  <Code>Public Const SalesCentreType As String = "1"
Public Const Accomodation As String = "41010"

Function ComparisonLabel() As String
   Return Report.Parameters!ComparisonType.Label &amp; IIf(Report.Parameters!ComparisonType.Value &gt; 4, " (Budget)", "")
End Function

Function LevelType(ByVal level As Integer) As String
    Select Case level
      Case 1
        Return "Department"
      Case 2
        Return "Roster Centre"
      Case 3
        Return "Centre"
      Case 4
        Return "Sub Centre"
      Case Else
        Return ""
    End Select
End Function

' % Factors need to be divided by 100 to display correctly.
Function FormatPercentage(ByVal factor As Decimal, ByVal kPITypeId As Integer) As Decimal
    Select Case kPITypeId
      Case 1, 6, 7, 8
        Return factor/100
      Case Else
        Return factor
    End Select
End Function

Function FactorInfo(ByVal centreTypeId As Integer, ByVal kPITypeId As Integer, ByVal factor2 As Decimal, ByVal factor3 As Decimal) As String
    If (centreTypeId = SalesCentreType) Then
      Return ""
    Else
      Select Case kPITypeId
        Case 1
          Return "of sales"
        Case 2
         Return "per room"
        Case 3
          Return "daily"
        Case 6
          Return "of site"
        Case 7
          Return "of House Profit"
        Case 8
          Return "% of GOP"
        Case 9
          Return  " + " &amp; FormatCurrency(factor2)
        Case 10
          Return "@ " &amp; FormatCurrency(factor3) &amp; " + " &amp; FormatCurrency(factor2)
        Case Else
          Return ""
      End Select
  End If
End Function

Function LevelColour(ByVal level As Integer) As String
    Select Case level
      Case 1
        Return "SteelBlue"
      Case 2
        Return "LightSteelBlue"
      Case 3
        Return "White"
      Case 4
        Return "White"
      Case Else
        Return ""
    End Select
End Function

Function LevelFont(ByVal level As Integer, ByVal centreTypeID As Integer) As String
    Select Case level
      Case 1
        Return "Bold"
      Case 2
        Return "Bold"
      Case 3
        If (centreTypeId = SalesCentreType) Then
          Return "Bold"
       Else 
          Return "Normal"
       End If
      Case 4
        If (centreTypeId = SalesCentreType) Then
          Return "Bold"
       Else 
          Return "Normal"
       End If
      Case Else
        Return "Bold"
    End Select

End Function

Function IsColumnHidden(ByVal index As Integer) As Boolean
    Return (index &gt;= Report.Parameters!SiteIDs.Count)
End Function

Function IsColumnHiddenToo(ByVal index As Integer, ByVal ToggleState As Boolean) As Boolean
    Return ((index &gt;= Report.Parameters!SiteIDs.Count) OR (ToggleState = False))
End Function

Function SiteName(ByVal index As Integer) As String
    Dim str As String = ""
    If (index &lt; Report.Parameters!SiteIDs.Count)
        str = Report.Parameters!SiteIDs.Label(index)
    End If
    Return str
End Function

Function SiteNameById(ByVal siteId As Integer) As String
    Dim str As String = ""
    Dim index As Integer = 0
    For i As Integer = 0 to Report.Parameters!SiteIDs.Count - 1
        If (Report.Parameters!SiteIDs.Value(i) = siteId) Then
            str = Report.Parameters!SiteIDs.Label(i)
            Exit For
        End If
    Next i
    Return str
End Function

Function IsSiteRow(ByVal index As Integer, ByVal siteId As Integer) As Boolean
    Dim valid As Boolean = False
    If (index &lt; Report.Parameters!SiteIDs.Count)
        valid = (Report.Parameters!SiteIDs.Value(index) = siteId)
    End If
    Return valid
End Function

Function SiteInfo(ByVal index As Integer, ByVal siteId As Integer, ByVal info As Object) As Object
' If the row belongs to the Site with the same SiteID 
' as index position in SiteIDs parameter,
' return the info for the site, else return Nothing.
' This filters the rows to only the data for the required SiteID.
    Return IIf(IsSiteRow(index, siteId), info, Nothing)
End Function

Function Fraction(ByVal numerator As Double, ByVal denominator As Double) As Double
    Return (numerator/denominator)
End Function

Function OrigItem(ByVal total As Double, ByVal quantity As Double, ByVal val As Double, ByVal availability As Integer, ByVal centreTypeId As Integer, ByVal accountCode As String, ByVal kPITypeId As Integer) As String
    Dim str As String = ""
    If (Report.Parameters!DisplayType.Value = 1) Then
        ' Sales CentreType displays Total, Non-Sales CentreType displays Estimated Spend
         str = FormatCurrency(IIf(centreTypeId = SalesCentreType, total, Fraction(total, quantity)), 2)
    Else
        ' Sales CentreType displays Quantity @ E.S., Non-Sales CentreType displays Factor
        If (centreTypeId = SalesCentreType) Then
            ' Accomodation displays Occupancy @ Estimated Spend,
            ' Other Sales show quantity @ Estimated Spend
            str = IIf(Trim(accountCode) = Accomodation, FormatPercent(Fraction(quantity, availability), 1), FormatNumber(quantity, 0)) &amp; " @ " &amp; FormatCurrency(Fraction(total, quantity), 2)
        Else
            Select Case kPITypeId
              Case 1, 6, 7, 8
                str = FormatPercent(val/100, 2)
              Case Else
                str = FormatCurrency(val, 2)
            End Select
        End If
    End If
    Return str
End Function

Function Item(ByVal total As Double, ByVal quantity As Double, ByVal val As Double, ByVal availability As Integer, ByVal centreTypeId As Integer, ByVal accountCode As String, ByVal kPITypeId As Integer) As String
    Dim str As String = ""
    ' Sales CentreType displays Quantity @ E.S.
    If (centreTypeId = SalesCentreType) Then
        ' Accomodation displays Occupancy @ Estimated Spend,
        ' Other Sales show quantity @ Estimated Spend
        str = IIf(Trim(accountCode) = Accomodation, FormatPercent(Fraction(quantity, availability), 1), FormatNumber(quantity, 0)) &amp; " @ " &amp; FormatCurrency(Fraction(total, quantity), 2)
    ' Non-Sales CentreType displays Factor or E.S
    Else
        Select Case kPITypeId
          Case 1, 6, 7, 8
            str = FormatPercent(val/100, 2)
          Case Else
              If (Report.Parameters!DisplayType.Value = 1) Then
                  str = FormatCurrency(val, 2)
              Else
                  str = FormatCurrency(Fraction(total, quantity), 2)
              End If
        End Select
    End If
    Return str
End Function

Function RowColour(ByVal centreTypeId As Integer, ByVal accountCode As String) As String
    Dim str As String = ""
    Select Case AccountType(centreTypeId, accountCode)
      Case 2
        str = "DarkOrange"
      Case 1
        str = "DarkOrange"
      Case 0
        str = "Black"
    End Select
    Return str
End Function

Function ToolTip(ByVal total As Double, ByVal quantity As Double, ByVal availability As Integer, ByVal accountCode As String, ByVal kpi As String) As String
    Dim str As String = ""
    str = "Total: " &amp; FormatCurrency(total, 2) &amp; " Qty: " &amp; FormatNumber(quantity, 0)
    If (Trim(accountCode) = Accomodation) Then
        str = str &amp;" Occupancy: " &amp; FormatPercent(Fraction(quantity, availability), 1)
    Else
        str = str &amp; " KPI: " &amp; kpi
    End If
    Return str
End Function

' Returns Integer identifying Row's account Type
' 2 = Accomodation (Sales CentreType)
' 1 Sales CentreType
' 0 Other
Function AccountType(ByVal centreTypeId As Integer, ByVal accountCode As String) As Integer
    If (centreTypeId = SalesCentreType) Then
        Return IIf(Trim(accountCode) = Accomodation, 2, 1)
    Else
        Return 0
    End If
End Function

Function LevelName(ByVal Level As Integer, ByVal Name As String) As String
    Return IIf(Level &gt; 2, Name, Nothing)
End Function</Code>
