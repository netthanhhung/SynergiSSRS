  <Code>Function LevelType(ByVal level As Integer) As String
    Select Case level
      Case 1
        Return "Department"
      Case 2
        Return "Roster Centre"
      Case 3
        Return "Centre"
      Case 4
        Return "Sub Centre"
      Case Else
        Return ""
    End Select
End Function

' Formats Factor in $ or %, based on KPI Type.
Function FactorFormat(ByVal kPITypeId As Integer) As String
    Select Case kPITypeId
      Case 1, 6, 7, 8
        Return "P"
      Case 2, 3, 9, 10
        Return "C"
      Case Else
        Return ""
    End Select
End Function

' % Factors need to be divided by 100 to display correctly.
Function FactorPercentage(ByVal factor As Decimal, ByVal kPITypeId As Integer) As Decimal
    Select Case kPITypeId
      Case 1, 6, 7, 8
        Return factor/100
      Case Else
        Return factor
    End Select
End Function

Function FactorInfo(ByVal kPITypeId As Integer, ByVal factor2 As Decimal, ByVal factor3 As Decimal) As String
    Select Case kPITypeId
      Case 1
        Return "% of Sales"
      Case 2
       Return "per room"
      Case 3
        Return "daily"
      Case 6
        Return "% of Site"
      Case 7
        Return "% of HOP"
      Case 8
        Return "% of GOP"
      Case 9
        Return  " + $" &amp; Cstr(factor2)
      Case 10
        Return "@ $" &amp; Cstr(factor3) &amp; " + $" &amp; Cstr(factor2)
      Case Else
        Return ""
    End Select
End Function

Function LevelColour(ByVal level As Integer) As String
    Select Case level
      Case 1
        Return "DarkGoldenRod"
      Case 2
        Return "GoldenRod"
      Case 3
        Return "PaleGoldenRod"
      Case 4
        Return "Khaki"
    End Select
End Function</Code>
