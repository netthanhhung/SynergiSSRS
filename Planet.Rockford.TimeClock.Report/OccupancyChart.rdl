  <Code>Function ExcludeFI2(ByVal RoomStr As String, ByVal FieldVal As Decimal) AS Decimal
	Return IIf(RoomStr &lt;&gt; "FI2", 0, FieldVal)
End Function

Function IsRowHidden(ByVal Apt As Boolean) As Boolean
    Return (Apt = "TRUE")
End Function

Function SelectRooms (ByVal  SumField As Double, ByVal ConditionField As String) As Double
    Return IIf(Left$(ConditionField, 1) = "[", 0, SumField)
End Function

Function AdjustAccom (ByVal  SumField As Double, ByVal ConditionField As String,  ByVal ExtraField As Double, ByVal Rooms As Integer, ByVal TotalRooms As Integer) As Double
    Return IIf((Left$(ConditionField, 1) = "[" AND Left$(ConditionField, 3) &lt;&gt; "[FN" AND Left$(ConditionField, 1) &lt;&gt; "[CAR"), 0, IIf(Left$(ConditionField, 1) = "[" , SumField, SumField + ExtraField*Rooms/TotalRooms))
End Function</Code>
